/* gcd of 2 numbers using recursion*/
#include<stdio.h>
#include<conio.h>
#include<process.h>
#include<stdlib.h>

int gcd(int a, int b)
{
if (b == 0)
return a;
int temp=a%b;
return gcd(b, temp);
}

void main()
{
	int a,b;
	printf("Enter two non-zero numbers\n");
    scanf("%d %d",&a,&b);
	printf("Gcd of two numbers is %d\n",gcd(a,b));
           
}
	
